<?xml version="1.0"?>
<program>
  <block>
    <decls>
      <var_decl>
        <sym id="1" name="char"/>
        <sym id="4" name="aaa"/>
      </var_decl>
      <var_decl>
        <sym id="2" name="int"/>
        <sym id="5" name="bbb"/>
      </var_decl>
      <var_decl>
        <sym id="3" name="float"/>
        <sym id="6" name="ccc"/>
      </var_decl>
    </decls>
    <stmts>
      <if>
        <varref>
          <sym id="4" name="aaa"/>
        </varref>
        <stmts>
          <return>
            <int value="5"/>
          </return>
        </stmts>
      </if>
      <if>
        <expr>
          <varref>
            <sym id="5" name="bbb"/>
          </varref>
          <op value="+"/>
          <int value="5"/>
        </expr>
        <stmts>
          <return>
            <int value="5"/>
          </return>
        </stmts>
        <stmts>
          <print>
            <varref>
              <sym id="4" name="aaa"/>
            </varref>
          </print>
        </stmts>
      </if>
      <while>
        <int value="2"/>
        <print>
          <int value="3"/>
        </print>
      </while>
    </stmts>
  </block>
</program>
