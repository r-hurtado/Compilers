// operator precedence and associativity
{
    int val;

    // associativity
    val = 1 == 2 == 3;
    val = 1 + 2 + 3 - 4 - 5 + 6;
    val = 1 * 2 * 3 / 4 / 5 % 6 % 7 / 8 * 9;

    // precedence
    val = 1 == 2 + 3 == 4;
    val = 1 == 2 - 3 == 4;
    val = 1 + 2 * 3 + 4;
    val = 1 - 2 * 3 - 4;
    val = 1 + 2 / 3 + 4;
    val = 1 - 2 / 3 - 4;
}


